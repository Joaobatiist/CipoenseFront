{
  "expo": {
    "name": "Evolution",
    "slug": "evolution",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/evolution.png",
    "scheme": "evolution",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joaaovictor.evolution",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/evolution.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joaaovictor.evolution"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Evolution - Sistema de Gest√£o Esportiva",
      "shortName": "Evolution",
      "lang": "pt-BR",
      "scope": "/",
      "themeColor": "#1c348e",
      "backgroundColor": "#f5f5f5",
      "display": "standalone",
      "orientation": "any"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/evolution.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "612964b9-ba4c-4e2d-8546-dd151c488f53"
      }
    }
  }
}
